{
  "compilerOptions": {
      "incremental": true,
      "target": "es5",
      "module": "commonjs",
      "declaration": true,
      "outDir": "lib",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": false,
      "strict": true,
      "noImplicitAny": true,
  },
  "include": [
      "src/**/*.ts",
  ],
}
